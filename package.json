{
  "name": "x-log",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -- turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "type-check": "turbo run type-check",
    "migrate": "dotenv -- turbo run migrate"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "dotenv-cli": "^11.0.0",
    "prettier": "^3.1.0",
    "turbo": "^2.6.3",
    "typescript": "^5.3.3"
  },
  "packageManager": "bun@1.0.0",
  "engines": {
    "bun": ">=1.0.0"
  }
}
